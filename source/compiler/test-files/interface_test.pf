newtype 

Foobarable = interface :
    foo(x self) -> int
    bar(x self) -> int

Qux = enum ZORT, TROZ, BLERP, SPONG

Fnug = clone int using +, -

Grunt = struct(flerp int, glerp Spoitable)

Derp = struct(blerp int)

Zort = abstract Derp/Grunt

Spoitable = interface :
    spoit(x self)

Thingable = interface :
    thing(x self, y list{string}) -> list{string}

Thongable = interface :
    thong(x self, y list{_ type}) -> list{_ type}

def

thong(z Fnug, L list{T type}) -> list{T type} :
    L

foo(q Qux) -> int :
    int q

bar(q Qux) -> int :
    int q

(p Qux) + (q Qux) -> Qux :
    Qux((int(p) + int(q)) mod 4)

xuq(z Zort) :
    z

troz(a Addable) :
    a

spoit(b Derp) :
    b

respoit(s Spoitable) :
    s 
